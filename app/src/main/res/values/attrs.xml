<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="TfTextView">
        <attr name="ftype">
            <enum name="regular" value="1" />
            <enum name="bold" value="2" />
        </attr>
        <attr name="isItalic" format="boolean" />
        <attr name="isUnderline" format="boolean" />
        <attr name="isStrikeThrough" format="boolean" />
    </declare-styleable>
    <declare-styleable name="SearchableSpinner">
        <attr name="hintText" format="string"/>

    </declare-styleable>

</resources>